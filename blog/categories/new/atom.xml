</span><span class='line'>
</span><span class='line'>&lt;p>import java.awt.&lt;em>;
</span><span class='line'>import java.awt.event.&lt;/em>;
</span><span class='line'>import javax.swing.*;&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>import util.SwingUtils;
</span><span class='line'>/&lt;em>*
</span><span class='line'> * 自定义 Dialog
</span><span class='line'> * @author 许楠钒
</span><span class='line'> * @date 2018/1/3
</span><span class='line'> &lt;/em>/&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>@SuppressWarnings(&ldquo;serial&rdquo;)
</span><span class='line'>public class DialogForClient extends JDialog implements ActionListener {&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;pre>&lt;code>final static int YES = 1, NO = 0;
</span><span class='line'>final static int width = 400;
</span><span class='line'>final static int height = 250;
</span><span class='line'>int message = -1;
</span><span class='line'>JButton Yes,No;
</span><span class='line'>static JTextArea area = new JTextArea(2,10);
</span><span class='line'>
</span><span class='line'>public DialogForClient(JFrame frame,String string) {
</span><span class='line'>
</span><span class='line'>    super(frame,string,true);
</span><span class='line'>
</span><span class='line'>    setLayout(new BorderLayout());
</span><span class='line'>    //SOUTH
</span><span class='line'>    JPanel pl_south = new JPanel();
</span><span class='line'>    Yes = new JButton("确认");
</span><span class='line'>    Yes.addActionListener(this);
</span><span class='line'>    No = new JButton("取消");
</span><span class='line'>    No.addActionListener(this);
</span><span class='line'>    pl_south.add(Yes);
</span><span class='line'>    pl_south.add(No);
</span><span class='line'>    add(pl_south,BorderLayout.SOUTH);
</span><span class='line'>    //CENTER
</span><span class='line'>    JScrollPane pl_center = new JScrollPane(area);  
</span><span class='line'>    area.setLineWrap(true);//字满自动换行
</span><span class='line'>    area.setWrapStyleWord(true);//换行不断字
</span><span class='line'>    area.setEditable(false);
</span><span class='line'>    area.setOpaque(false);
</span><span class='line'>    add(pl_center,BorderLayout.CENTER);
</span><span class='line'>    //设置dialog大小以及位置
</span><span class='line'>    setSize(width, height);
</span><span class='line'>    SwingUtils.popDialogInCenterOfScreen(this);
</span><span class='line'>    addWindowListener(new WindowAdapter() {
</span><span class='line'>        public void windowClosing(WindowEvent e) {
</span><span class='line'>            message = -1;
</span><span class='line'>            setVisible(false);
</span><span class='line'>        }
</span><span class='line'>    });
</span><span class='line'>}
</span><span class='line'>//设置按钮事件
</span><span class='line'>public void actionPerformed(ActionEvent e) {
</span><span class='line'>    if(e.getSource() == Yes) {
</span><span class='line'>        message = YES;
</span><span class='line'>        setVisible(false);
</span><span class='line'>    }
</span><span class='line'>    else if(e.getSource() == No) {
</span><span class='line'>        message = NO;
</span><span class='line'>        setVisible(false);
</span><span class='line'>    }
</span><span class='line'>}
</span><span class='line'>//获取按钮标识
</span><span class='line'>public int getMessage() {
</span><span class='line'>    return message;
</span><span class='line'>}
</span><span class='line'>//设置要提示的信息
</span><span class='line'>public void setInf(String s) {
</span><span class='line'>    area.setText(s);
</span><span class='line'>}
